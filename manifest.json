{
  "manifest_version": 3,
  "name": "Redirect Ranger",
  "version": "1.1",
  "description": "Automatically close redirect tabs in Chrome",
  "permissions": ["tabs"],
  "action": {
    "default_icon": {
      "16": "icon16.png",
      "48": "icon48.png",
      "128": "icon128.png"
    }
  },
  "background": {
    "service_worker": "background.js"
  },
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  }
}
